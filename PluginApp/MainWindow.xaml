<Window x:Class="PluginApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:pluginApp="clr-namespace:PluginApp"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="75"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="518" Height="247">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="150"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <pluginApp:ListingPanel x:Name="listingPanel" Grid.Column="0"/>
            <pluginApp:ExecutionPanel x:Name="executionPanel" Grid.Column="1"/>
        </Grid>
    </Grid>
</Window>
